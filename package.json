    "@astrojs/preact": "^3.5.3",
    "@astrojs/react": "^3.6.2",
    "@astrojs/starlight": "^0.28.4",
    "@astrojs/svelte": "^5.7.2",
    "astro": "^4.16.8",
    "preact": "^10.24.3",
    "sharp": "^0.33.5",
    "svelte": "^5.1.6",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.2.7"
  }
